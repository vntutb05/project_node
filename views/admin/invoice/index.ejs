
  <div class="container-fluid">
    <div class="card shadow mb-4">
      <div class="card-header py-3">
        <h6 class="m-0 font-weight-bold text-primary">Invoices List</h6>
      </div>
      <div class="card-body">
        <div class="table-responsive">
          <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
            <thead>
              <tr>
                <th>Fullname</th>
                <th>Address</th>
                <th>Phone</th>
                <th>Total</th>
                <th style="width: 20%;">Action</th>
              </tr>
            </thead>
            <tfoot>
              <tr>
                <th>Fullname</th>
                <th>Address</th>
                <th>Phone</th>
                <th>Total</th>
                <th>Action</th>
              </tr>
            </tfoot>
            <tbody>
              <%data.cart.forEach(function(item){%>
                <tr>
                  <td><%= item.infoCustomer.name%></td>
                  <td><%= item.infoCustomer.address%></td>
                  <td><%= item.infoCustomer.phone%></td>
                  <td><%= item.total%></td>
                  <td>
                        <a href="/admin/invoice/detail/<%=item._id%>" class="alert alert-success" style="margin: 3px">Detail</a>
                  </td>
                </tr>
                <%})%>
            </tbody>
          </table>
      </div>
  </div>
