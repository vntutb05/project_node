<%- include("../../layout/admin/head")%>

<%- include("../../layout/admin/slidebar")%>

<%- include("../../layout/admin/topbar")%>
  <div class="container-fluid">
          <div class="card shadow mb-4">
            <div class="card-header py-3">
              <h6 class="m-0 font-weight-bold text-primary">Products List</h6>
            </div>
            <div class="card-body">
              <div class="table-responsive">
                <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
                  <thead>
                    <tr>
                      <th>Name</th>
                      <th>Category</th>
                      <th>Tag</th>
                      <th>Price</th>
                      <th>Sale Price</th>
                      <th>Decription</th>
                      <th>Start upload</th>
                      <th>Action</th>
                    </tr>
                  </thead>
                  <tfoot>
                    <tr>
                      <th>Name</th>
                      <th>Category</th>
                      <th>Tag</th>
                      <th>Price</th>
                      <th>Sale Price</th>
                      <th>Decription</th>
                      <th>Start upload</th>
                      <th>Action</th>
                    </tr>
                  </tfoot>
                  <tbody>
                    <%data.forEach(function(item){%>
                      <tr>
                        <td><%= item.name%></td>
                        <td>Danh má»¥c 1</td>
                        <td><%= item.tag%></td>
                        <td><%= item.price%></td>
                        <td><%= item.sale%></td>
                        <td><%= item.description%></td>
                        <td>2011/04/25</td>
                        <td>
                              <a href="/admin/product/edit/<%=item._id%>" class="alert alert-primary">Edit</a>
                              <a href="/admin/product/delete/<%=item._id%>" class="alert alert-danger">Delete</a>
                        </td>
                      </tr>
                    <%})%>
                   
                  </tbody>
                </table>
            </div>
        </div>
<%- include("../../layout/admin/footer")%>
